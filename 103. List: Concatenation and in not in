# operation list
# +
list1 = [1,2,3]
list2 = [8,9,10]
print(list1+list2)
print(list1)
print(list2)
list1.extend([4,5,6])
print(list1)
list2 =list2+[11,12,13]
print(list2)
# *
list1 =[1,2,3]
print(list1*2)
print(list1)
print(list1*3)
# in
print(1 in list1)
if 3 in list1:
    print('Found')
else:
    print('Not Found')
#
if 5 in list1:
    print('Found')
else:
    print('Not Found')

