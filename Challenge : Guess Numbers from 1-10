import random
n=random.randint(1,10)
guess=0
while guess != n:
   guess= int(input('Guess number'))
   if guess<n:
      print('it is smaller')
   elif guess > n:
      print('it is larger')
   else:
      print('correct guess')

# output
Guess number2
it is larger
Guess number1
correct guess
Guess number7
it is larger
Guess number9
it is larger
Guess number3
it is smaller
Guess number8
it is larger
Guess number6
correct guess
