# pop method to delete the element
L1 = [5,6,7,8,9]
print(L1.pop())
print(L1)
print(L1.pop(0))
print(L1)
print(L1.pop(1))
print(L1)

# delete
L1 = [5,6,7,8,9]
del L1[3]
print(L1)
del L1[0:2]
print(L1)

# remove method
L1 = [5,6,7,5,6,7]
L1.remove(6)
print(L1)
L1.clear
print(L1)
#
L1 = [5,6,7,5,6,7]
del L1[:]
print(L1)

# output
9
[5, 6, 7, 8]
5
[6, 7, 8]
7
[6, 8]
[5, 6, 7, 9]
[7, 9]
[5, 7, 5, 6, 7]
[5, 7, 5, 6, 7]
[]
