# int example data type
a=125
print(type(a)) # <class 'int'>
print(a)
b=1246
print(b) 
x=12345678889012345678890123456790
print(x) #12345678889012345678890123456790 output
y=-17
print(y) # -17 output
print(x.__sizeof__()) # 40 out  put
print(y.__sizeof__()) # 28 out put means size or bytes 

# floating data types
a=12.59
print(a) # 12.59
b=1259E-2
print(b) # out put 12.59
c=0.1259e2
print(c) # out put 12.59
