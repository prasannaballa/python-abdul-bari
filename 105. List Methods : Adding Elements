# List Methods
# append
L1 = [5,6,7,8,9]
print(len(L1))
L1.append(10)
print(len(L1))
print(L1)

#
L1 = [5,6,7,8,9]
L1[len(L1):] = [10]
print(L1)
L1[6:6]=[11]
print(L1)

# extend
L1 = [5,6,7,8,9]
L1.extend([10,11,12])
print(L1)
L1.extend('abc')
print(L1)
L1 = [5,6,7,8,9]
L1[len(L1):]=[10,11,12]
print(L1)
# insert method used for inserting any element
L1 = [5,6,7,8,9]
print(id(L1))
L1.insert(0,10)
print(L1)
print(id(L1))
L1.insert(3,20)
print(L1)
L1[4:4] =[22]
print(L1)
L1[0:0] =[25]
print(L1)

# copy
L1 = [5,6,7,8,9]
L2=L1.copy()
print(L2)
print(id(L1))
print(id(L2))
# output here
5
6
[5, 6, 7, 8, 9, 10]
[5, 6, 7, 8, 9, 10]
[5, 6, 7, 8, 9, 10, 11]
[5, 6, 7, 8, 9, 10, 11, 12]
[5, 6, 7, 8, 9, 10, 11, 12, 'a', 'b', 'c']
[5, 6, 7, 8, 9, 10, 11, 12]
