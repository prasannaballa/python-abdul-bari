# check if a given lower charcteris a vowel or constant
jhon= float(input("Enter jhon's age"))
smith= float(input("Enter smith's age"))
ajay= float(input("Enter ajay's age"))
if jhon>smith>ajay:
    print('jhon is elder')
else: 
   if smith>ajay:
    print('smith is elder')
   else:
     print('ajay id elder')

# elif condition
jhon= float(input("Enter jhon's age"))
smith= float(input("Enter smith's age"))
ajay= float(input("Enter ajay's age"))
if jhon>smith>ajay:
    print('jhon is elder')
elif smith>ajay:
    print('smith is elder')
else:
     print('ajay id elder')
