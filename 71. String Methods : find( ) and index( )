# find it countgs left hand side
s= 'hello, how are you'
print(s.find('o'))
print(s.find('o',5))
print(s.find('k')) # find gives -1 and print(s.index('k')) index gives value error subscrption not found
print(s.find('how'))

# rfind it counts right hand side
print(s.rfind('o'))
print(s.rfind('o',15))
print(s.rfind('o',0,15)) # before 16 
print(s.rfind('k'))

# outputs
4
8
-1
7
16
16
8
-1
# count
print(s.count('me'))
print(s.count('how'))
print(s.count('o'))

# output
0
1
3
