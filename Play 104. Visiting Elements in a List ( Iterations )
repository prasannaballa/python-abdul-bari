# lterating a List
# for loop
list1 = [5,6,7,8,9]
for x in list1:
    print(x)

for i in range(len(list1)):
    print(list1[i])

for i in range(2,len(list1)):
    print(list1[i])

# for loop using range 
for i in range(len(list1)-1,-1,-1):
    print(list1[i])

#  while loop
i = 0
while i< len(list1):
    print(list1[i])
    i = i+1
# output here
5
6
7
8
9
