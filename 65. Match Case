


for i in range(0,5):
  print('*'*5) # ('*')

# 
for i in range (5,0,-1):
  print('*'*i)

# match case
day = int(input('Enter Day Number'))

if day == 1:
  print('Sunday')
elif day == 2:
  print('Monday')
elif day == 3:
  print('Tuesday')
elif day == 4:
  print('Wednesday')
elif day == 5:
  print('Thursday')
elif day == 6:
  print('Friday')
elif day == 7:
  print('Saturday')
else:
  print('Holiday')

# output
