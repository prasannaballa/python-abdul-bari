while True:
    n= int(input('Enter a number'))
    if n>0:
        print('postive')
    elif n<0:
        print('negative') # program never stopm 
# how to stop program? using break we can stop it
while True:
    n= int(input('Enter a number'))
    if n>0:
        print('postive')
    elif n<0:
        print('negative')
    else:
      break;

# output
Enter a number4
postive       
Enter a number-7
negative      
Enter a number8
postive       
Enter a number-9
negative      
Enter a number0

# # contine
count=0
while count<10:
    n= int(input('Enter a number'))
    print(n)
    count += 1
# output we will get 10 values


# contine
count=0
while count<10:
    n= int(input('Enter a number'))
    if n%3 == 0:
        continue
    print(n)
    # divisible by 3 and values max numers answers 10 values we get countiue use for logical 
